{
  "name": "@barelyhuman/range",
  "version": "0.1.1",
  "packageManager": "pnpm@7.1.5",
  "author": "Reaper <ahoy@barelyhuman.dev>",
  "license": "MIT",
  "keywords": [
    "range",
    "in range",
    "range maps",
    "range blockers"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "next": "bumpp --commit --tag",
    "test": "uvu -r tsm tests",
    "lint": "eslint .",
    "fix": "eslint . --fix ",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.24.2",
    "@antfu/ni": "^0.16.2",
    "@arkweid/lefthook": "^0.7.7",
    "@types/node": "^17.0.35",
    "bumpp": "^7.1.1",
    "bundt": "^1.1.5",
    "eslint": "^8.16.0",
    "esno": "^0.16.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tsm": "^2.2.1",
    "typescript": "^4.7.2",
    "unbuild": "^0.7.4",
    "uvu": "^0.5.3"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "*.d.ts"
    ],
    "extends": [
      "@antfu"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "terser": {
    "compress": {
      "passes": 3,
      "pure_getters": true
    },
    "mangle": {
      "reserved": [
        "__shared__"
      ],
      "toplevel": true
    }
  },
  "umd:name": "range"
}
